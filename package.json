{
    "name": "acav-we",
    "version": "1.0.0",
    "author": "aleab (https://github.com/aleab)",
    "homepage": "https://aleab.github.io/acav-we",
    "license": "UNLICENSED",
    "private": true,
    "repository": "github:aleab/acav-we#gh-pages.src",
    "scripts": {
        "clean": "rimraf ./dist",
        "prebuild": "npm run clean && npm run type-check",
        "prebuild:dev": "npm run clean",
        "build": "cross-env NODE_ENV=production webpack --node-env production",
        "build:dev": "cross-env NODE_ENV=development webpack --node-env development",
        "postbuild": "npm run purgecss",
        "postbuild:dev": "npm run purgecss",
        "start": "webpack serve --config ./webpack.config.js --mode development",
        "test": "npm run type-check",
        "purgecss": "purgecss --config ./purgecss.config.js --output ./dist",
        "type-check": "tsc --noEmit",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist --dotfiles"
    },
    "dependencies": {
        "@babel/runtime": "^7.13.10",
        "@primer/octicons-react": "^12.1.0",
        "milligram": "^1.4.1",
        "normalize.css": "^8.0.1",
        "qs": "^6.10.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "whatwg-fetch": "^3.6.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.13.10",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@types/qs": "^6.9.6",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/react-router-dom": "^5.1.7",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^8.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.1.3",
        "cssnano": "^4.1.10",
        "eslint": "^7.22.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.5.2",
        "gh-pages": "^3.1.0",
        "mini-css-extract-plugin": "^1.3.9",
        "postcss-loader": "^5.2.0",
        "purgecss": "^4.0.2",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^5.1.1",
        "typescript": "^4.2.3",
        "webpack": "^5.27.1",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-dotenv-plugin": "^2.1.0"
    }
}
