{
    "name": "acav-we",
    "displayName": "aCAV-WE",
    "version": "1.6.3",
    "description": "A highly customizable audio visualizer for Wallpaper Engine",
    "author": "aleab (https://github.com/aleab)",
    "private": true,
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/aleab/acav-we"
    },
    "scripts": {
        "update-latest": "ncu -u && npm install",
        "change-version": "node ./build-scripts/change-version.js",
        "clean": "rimraf ./dist/**/* ./bundle-report.html ./bundle-stats.json",
        "distclean": "npm run clean && rimraf ./package-lock.json \"./node_modules/!(rimraf|.bin)\"",
        "prebuild": "npm run clean && npm run test && babel-node ./build-scripts/pre-build.js",
        "prebuild:dev": "npm run prebuild",
        "build": "webpack --node-env production",
        "build:dev": "webpack --node-env development",
        "start:dev": "npm run build:dev && webpack serve --config ./webpack.config.js --mode development",
        "start:we": "npm run pre-build && babel-node ./webpack-start-we.js",
        "test": "npm run test:tsc && npm run test:lint",
        "test:tsc": "tsc --noEmit --project .",
        "test:lint": "eslint -c .eslintrc --ext .js,.jsx,.ts,.tsx,.mjs ./src"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4",
        "@xstate/react": "^1.5.1",
        "chai": "^4.3.4",
        "color-convert": "^2.0.1",
        "fuse.js": "^6.4.6",
        "html2canvas": "github:aleab/html2canvas#background-parser",
        "idb": "^6.1.2",
        "lodash": "^4.17.21",
        "musicbrainz-api": "github:aleab/musicbrainz-api#acav-we",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "simplebar": "^5.3.5",
        "simplebar-react": "^2.3.5",
        "three": "^0.132.2",
        "xstate": "^4.23.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.15.0",
        "@babel/node": "^7.14.9",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@babel/standalone": "^7.15.3",
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "@types/chai": "^4.2.21",
        "@types/color-convert": "^2.0.0",
        "@types/html2canvas": "0.0.36",
        "@types/lodash": "^4.14.172",
        "@types/offscreencanvas": "^2019.6.3",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/three": "^0.131.0",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "@yushijinhun/three-minifier-webpack": "^0.3.0",
        "assert": "^2.0.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-lodash": "^3.3.4",
        "chalk": "^4.1.2",
        "copy-webpack-plugin": "^9.0.1",
        "core-js": "^3.16.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "cssnano": "^5.0.8",
        "cssnano-preset-default": "^5.1.4",
        "dotenv-webpack": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^3.0.1",
        "glob": "^7.1.7",
        "html-react-parser": "^1.2.8",
        "jsonc-parser": "^3.0.0",
        "license-webpack-plugin": "^2.3.21",
        "lodash-webpack-plugin": "^0.11.6",
        "mini-css-extract-plugin": "^2.2.0",
        "npm-check-updates": "^11.8.3",
        "postcss": "^8.3.6",
        "postcss-import": "^14.0.2",
        "postcss-loader": "^6.1.1",
        "rimraf": "latest",
        "style-loader": "^3.2.1",
        "svg-react-loader": "^0.4.6",
        "terser-webpack-plugin": "^5.1.4",
        "typescript": "^4.4.2",
        "utility-types": "^3.10.0",
        "webpack": "^5.51.1",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.0.0"
    }
}
