{
    "name": "acav-we",
    "version": "1.0.0",
    "author": "aleab (https://github.com/aleab)",
    "homepage": "https://aleab.github.io/acav-we",
    "license": "UNLICENSED",
    "private": true,
    "repository": "github:aleab/acav-we#gh-pages.src",
    "scripts": {
        "clean": "rimraf ./dist",
        "prebuild": "npm run clean && npm run type-check",
        "prebuild:dev": "npm run clean",
        "build": "cross-env NODE_ENV=production webpack --node-env production",
        "build:dev": "cross-env NODE_ENV=development webpack --node-env development",
        "postbuild": "npm run purgecss",
        "postbuild:dev": "npm run purgecss",
        "start": "webpack serve --config ./webpack.config.js --mode development",
        "test": "npm run type-check",
        "purgecss": "purgecss --config ./purgecss.config.js --output ./dist",
        "type-check": "tsc --noEmit",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist --dotfiles"
    },
    "dependencies": {
        "@babel/runtime": "^7.20.1",
        "@primer/octicons-react": "^12.1.0",
        "milligram": "^1.4.1",
        "normalize.css": "^8.0.1",
        "qs": "^6.11.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "whatwg-fetch": "^3.6.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.2",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@types/qs": "^6.9.7",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "cssnano": "^5.1.14",
        "eslint": "^8.27.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-webpack-plugin": "^3.2.0",
        "gh-pages": "^4.0.0",
        "mini-css-extract-plugin": "^2.6.1",
        "postcss-loader": "^7.0.1",
        "purgecss": "^5.0.0",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^5.3.6",
        "typescript": "^4.8.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-dotenv-plugin": "^2.1.0"
    }
}
