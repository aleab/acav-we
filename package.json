        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/standalone": "^7.13.8",
        "@fullhuman/postcss-purgecss": "^2.3.0",
        "@types/chai": "^4.2.15",
