{
    "name": "acav-we",
    "displayName": "aCAV-WE",
    "version": "1.6.4",
    "description": "A highly customizable audio visualizer for Wallpaper Engine",
    "author": "aleab (https://github.com/aleab)",
    "private": true,
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/aleab/acav-we"
    },
    "scripts": {
        "update-latest": "ncu -x chalk -u && npm install",
        "change-version": "node ./build-scripts/change-version.js",
        "clean": "rimraf ./dist/**/* ./bundle-report.html ./bundle-stats.json",
        "distclean": "npm run clean && rimraf ./package-lock.json \"./node_modules/!(rimraf|.bin)\"",
        "prebuild": "npm run clean && npm run test && babel-node ./build-scripts/pre-build.js",
        "prebuild:dev": "npm run prebuild",
        "build": "webpack --node-env production",
        "build:dev": "webpack --node-env development",
        "start:dev": "npm run build:dev && webpack serve --config ./webpack.config.js --mode development",
        "start:we": "npm run pre-build && babel-node ./webpack-start-we.js",
        "test": "npm run test:tsc && npm run test:lint",
        "test:tsc": "tsc --noEmit --project .",
        "test:lint": "eslint -c .eslintrc --ext .js,.jsx,.ts,.tsx,.mjs ./src"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4",
        "@xstate/react": "^1.6.3",
        "chai": "^4.3.4",
        "color-convert": "^2.0.1",
        "fuse.js": "^6.5.3",
        "html2canvas": "github:aleab/html2canvas#background-parser",
        "idb": "^7.0.0",
        "lodash": "^4.17.21",
        "musicbrainz-api": "github:aleab/musicbrainz-api#acav-we",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "simplebar": "^5.3.6",
        "simplebar-react": "^2.3.6",
        "three": "^0.136.0",
        "xstate": "^4.27.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.8",
        "@babel/core": "^7.16.7",
        "@babel/node": "^7.16.8",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.16.8",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/standalone": "^7.16.9",
        "@fullhuman/postcss-purgecss": "^4.1.3",
        "@types/chai": "^4.3.0",
        "@types/color-convert": "^2.0.0",
        "@types/html2canvas": "0.0.36",
        "@types/lodash": "^4.14.178",
        "@types/offscreencanvas": "^2019.6.4",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/three": "^0.136.1",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "@yushijinhun/three-minifier-webpack": "^0.3.1",
        "assert": "^2.0.0",
        "babel-loader": "^8.2.3",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-lodash": "^3.3.4",
        "chalk": "^4.1.2",
        "copy-webpack-plugin": "^10.2.0",
        "core-js": "^3.20.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "cssnano": "^5.0.15",
        "cssnano-preset-default": "^5.1.10",
        "dotenv-webpack": "^7.0.3",
        "eslint": "^8.6.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-webpack-plugin": "^3.1.1",
        "glob": "^7.2.0",
        "html-react-parser": "^1.4.5",
        "jsonc-parser": "^3.0.0",
        "license-webpack-plugin": "^4.0.0",
        "lodash-webpack-plugin": "^0.11.6",
        "mini-css-extract-plugin": "^2.4.6",
        "npm-check-updates": "^12.1.0",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "postcss-loader": "^6.2.1",
        "rimraf": "latest",
        "style-loader": "^3.3.1",
        "svg-react-loader": "^0.4.6",
        "terser-webpack-plugin": "^5.3.0",
        "typescript": "^4.5.4",
        "url": "^0.11.0",
        "utility-types": "^3.10.0",
        "webpack": "^5.65.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.3"
    }
}
