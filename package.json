{
    "name": "acav-we",
    "displayName": "aCAV-WE",
    "version": "0.1.0",
    "description": "A highly customizable audio visualizer for Wallpaper Engine",
    "author": "aleab (https://github.com/aleab)",
    "private": true,
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/aleab/acav-we"
    },
    "scripts": {
        "clean": "run-script-os",
        "clean:windows": "if exist dist (del /S /Q dist\\* && for /D %d in (dist\\*) do rmdir /S /Q \"%d\")",
        "clean:nix": "[ -d dist ] && rm -r ./dist/*",
        "prebuild": "npm run clean && babel-node ./build-scripts/pre-build.js",
        "prebuild:dev": "npm run prebuild",
        "build": "cross-env NODE_ENV=production webpack -p",
        "build:dev": "cross-env NODE_ENV=development webpack --mode development",
        "start:dev": "npm run build:dev && webpack-dev-server --mode development",
        "start:we": "npm run pre-build && babel-node ./webpack-start-we.js",
        "lint": "eslint .",
        "fi": "run-script-os && npm install && npm dedupe",
        "fi:windows": "(rmdir /S /Q node_modules & del /S /Q package-lock.json) 2>NUL",
        "fi:nix": "rm -rf node_modules package-lock.json"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
        "@fortawesome/free-brands-svg-icons": "^5.13.0",
        "@fortawesome/free-solid-svg-icons": "^5.13.0",
        "@fortawesome/react-fontawesome": "^0.1.9",
        "@xstate/react": "^0.8.1",
        "color-convert": "^2.0.1",
        "fuse.js": "^5.0.10-beta",
        "html2canvas": "^1.0.0-rc.5",
        "idb": "^5.0.2",
        "lodash": "^4.17.15",
        "musicbrainz-api": "github:aleab/musicbrainz-api#acav-we",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "xstate": "^4.8.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/node": "^7.8.7",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@babel/standalone": "^7.9.4",
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@types/color-convert": "^1.9.0",
        "@types/html2canvas": "0.0.35",
        "@types/lodash": "^4.14.149",
        "@types/react": "^16.9.31",
        "@types/react-dom": "^16.9.6",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-dynamic-import-node": "^2.3.0",
        "chalk": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "latest",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "cssnano-preset-default": "^4.0.7",
        "dotenv-webpack": "^1.7.0",
        "eslint": "^6.1.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "file-loader": "^6.0.0",
        "html-react-parser": "^0.10.3",
        "jsonc-parser": "^2.2.1",
        "license-webpack-plugin": "^2.1.4",
        "mini-css-extract-plugin": "^0.9.0",
        "postcss-loader": "^3.0.0",
        "run-script-os": "latest",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "ts-loader": "^6.2.2",
        "typescript": "^3.8.3",
        "utility-types": "^3.10.0",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
