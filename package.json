{
    "name": "acav-we",
    "displayName": "aCAV-WE",
    "version": "1.4.1",
    "description": "A highly customizable audio visualizer for Wallpaper Engine",
    "author": "aleab (https://github.com/aleab)",
    "private": true,
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/aleab/acav-we"
    },
    "scripts": {
        "clean": "rimraf ./dist/**/* ./bundle-report.html ./bundle-stats.json",
        "distclean": "npm run clean && rimraf ./package-lock.json \"./node_modules/!(rimraf|.bin)\"",
        "prebuild": "npm run clean && babel-node ./build-scripts/pre-build.js",
        "prebuild:dev": "npm run prebuild",
        "build": "cross-env NODE_ENV=production webpack -p",
        "build:dev": "cross-env NODE_ENV=development webpack --mode development",
        "start:dev": "npm run build:dev && webpack-dev-server --mode development",
        "start:we": "npm run pre-build && babel-node ./webpack-start-we.js",
        "lint": "eslint -c .eslintrc ./src/**/*.ts ./src/**/*.tsx ./src/**/*.js"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.2",
