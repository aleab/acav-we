{
    "name": "acav-we",
    "version": "1.0.0",
    "author": "aleab (https://github.com/aleab)",
    "homepage": "https://aleab.github.io/acav-we",
    "license": "UNLICENSED",
    "private": true,
    "repository": "github:aleab/acav-we#gh-pages.src",
    "scripts": {
        "clean": "rimraf ./dist",
        "prebuild": "npm run clean && npm run type-check",
        "prebuild:dev": "npm run clean",
        "build": "cross-env NODE_ENV=production webpack -p --mode production --display-modules",
        "build:dev": "cross-env NODE_ENV=development webpack --mode development --display-modules",
        "postbuild": "npm run purgecss",
        "postbuild:dev": "npm run purgecss",
        "start": "webpack-dev-server --mode development",
        "test": "npm run type-check",
        "purgecss": "purgecss --config ./purgecss.config.js --output ./dist",
        "type-check": "tsc --noEmit",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist --dotfiles"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.2",
        "@primer/octicons": "^9.6.0",
        "milligram": "^1.3.0",
        "normalize.css": "^8.0.1",
        "qs": "^6.9.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@types/qs": "^6.9.1",
        "@types/react": "^16.9.27",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "babel-loader": "^8.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.2",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "eslint": "^6.1.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "file-loader": "^6.0.0",
        "gh-pages": "^2.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "postcss-loader": "^3.0.0",
        "purgecss": "^2.1.0",
        "rimraf": "^3.0.2",
        "svg-react-loader": "^0.4.6",
        "terser-webpack-plugin": "^2.3.5",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-dotenv-plugin": "^2.1.0"
    }
}
