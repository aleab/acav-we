{
    "name": "acav-we",
    "displayName": "aCAV-WE",
    "version": "1.6.0",
    "description": "A highly customizable audio visualizer for Wallpaper Engine",
    "author": "aleab (https://github.com/aleab)",
    "private": true,
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/aleab/acav-we"
    },
    "scripts": {
        "update-latest": "ncu -u && npm install",
        "clean": "rimraf ./dist/**/* ./bundle-report.html ./bundle-stats.json",
        "distclean": "npm run clean && rimraf ./package-lock.json \"./node_modules/!(rimraf|.bin)\"",
        "prebuild": "npm run clean && npm run test && babel-node ./build-scripts/pre-build.js",
        "prebuild:dev": "npm run prebuild",
        "build": "webpack --node-env production",
        "build:dev": "webpack --node-env development",
        "start:dev": "npm run build:dev && webpack serve --config ./webpack.config.js --mode development",
        "start:we": "npm run pre-build && babel-node ./webpack-start-we.js",
        "test": "npm run test:tsc && npm run test:lint",
        "test:tsc": "tsc --noEmit --project .",
        "test:lint": "eslint -c .eslintrc --ext .js,.jsx,.ts,.tsx,.mjs ./src"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.3",
        "@xstate/react": "^1.3.1",
        "chai": "^4.3.4",
        "color-convert": "^2.0.1",
        "fuse.js": "^6.4.6",
        "html2canvas": "github:aleab/html2canvas#background-parser",
        "idb": "^6.0.0",
        "lodash": "^4.17.21",
        "musicbrainz-api": "github:aleab/musicbrainz-api#acav-we",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "simplebar": "^5.3.0",
        "simplebar-react": "^2.3.0",
        "three": "^0.126.1",
        "xstate": "^4.17.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.13.10",
        "@babel/node": "^7.13.12",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.13.12",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/standalone": "^7.13.12",
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "@types/chai": "^4.2.15",
        "@types/color-convert": "^2.0.0",
        "@types/html2canvas": "0.0.36",
        "@types/lodash": "^4.14.168",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/three": "^0.126.1",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "@yushijinhun/three-minifier-webpack": "^0.3.0-alpha.0",
        "assert": "^2.0.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-lodash": "^3.3.4",
        "chalk": "^4.1.0",
        "copy-webpack-plugin": "^8.1.0",
        "core-js": "^3.9.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.1.3",
        "cssnano": "^4.1.10",
        "cssnano-preset-default": "^4.0.7",
        "dotenv-webpack": "^7.0.2",
        "eslint": "^7.22.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.5.2",
        "glob": "^7.1.6",
        "html-react-parser": "^1.2.4",
        "jsonc-parser": "^3.0.0",
        "license-webpack-plugin": "^2.3.15",
        "lodash-webpack-plugin": "^0.11.6",
        "mini-css-extract-plugin": "^1.3.9",
        "npm-check-updates": "^11.3.0",
        "postcss-import": "^14.0.0",
        "postcss-loader": "^5.2.0",
        "rimraf": "latest",
        "style-loader": "^2.0.0",
        "svg-react-loader": "^0.4.6",
        "terser-webpack-plugin": "^5.1.1",
        "typescript": "^4.2.3",
        "utility-types": "^3.10.0",
        "webpack": "^5.27.2",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    }
}
