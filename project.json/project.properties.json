{
    "schemecolor": {
        "order": 0,
        "text": "ui_browse_properties_scheme_color",
        "type": "color",
        "value": "0 0 0"
    },
    "showDescriptions": {
        "order": 1,
        "type": "bool",
        "text": "ui_showDescriptions",
        "value": true
    },
    "showAdvancedOptions": {
        "order": 2,
        "type": "bool",
        "text": "ui_showAdvancedOptions",
        "value": false
    },

    // [AUDIO SAMPLES]
    "audioSamples": {
        "order": 20,
        "type": "text",
        "text": "ui_audioSamples"
    },
    "$_audioSamples": {
        "order": 21,
        "type": "text",
        "text": "ui_$_audioSamples",
        "condition": "(showAdvancedOptions.value && showDescriptions.value)"
    },
    "audioSamples_correct": {
        "order": 22,
        "type": "bool",
        "text": "ui_audioSamples_correct",
        "value": true,
        "condition": "showAdvancedOptions.value"
    },
    "audioSamples_volumeGain": {
        "order": 24,
        "type": "slider",
        "text": "ui_audioSamples_volumeGain",
        "value": 35,
        "min": -100,
        "max": 100,
        "editable": false
    },
    "audioSamples_freqThreshold": {
        "order": 25,
        "type": "slider",
        "text": "ui_audioSamples_freqThreshold",
        "value": 0.1,
        "min": 0,
        "max": 10,
        "fraction": true,
        "step": 0.1,
        "editable": false
    },
    "audioSamples_scale": {
        "order": 26,
        "type": "combo",
        "text": "ui_audioSamples_scale",
        "options": [
            { "label": "x", "value": "Linear" },
            { "label": "x<sup>2</sup>", "value": "Square" },
            { "label": "sqrt(x)", "value": "Sqrt" },
            { "label": "log(x+1)", "value": "Log" },
            { "label": "a&#8201;log(x+1) + b&#8201;x<sup>2</sup>", "value": "Log$Square" },
            { "label": "Gaussian", "value": "Gaussian" }
        ],
        "value": "Log",
        "condition": "showAdvancedOptions.value"
    },

    "$_audioSamples_scale_Square": {
        "order": 27,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Square",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Square'"
    },
    "$_audioSamples_scale_Sqrt": {
        "order": 28,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Sqrt",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Sqrt'"
    },
    "$_audioSamples_scale_Log": {
        "order": 29,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Log",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Log'"
    },
    "$_audioSamples_scale_Log$Square": {
        "order": 30,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Log$Square",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Log$Square'"
    },
    "$_audioSamples_scale_Gaussian": {
        "order": 31,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Gaussian",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Gaussian'"
    },

    "audioSamples_buffer": {
        "order": 35,
        "type": "slider",
        "text": "ui_audioSamples_buffer",
        "value": 0.4,
        "min": 0,
        "max": 1,
        "fraction": true,
        "step": 0.1,
        "editable": false,
        "condition": "showAdvancedOptions.value"
    },
    "$_audioSamples_buffer": {
        "order": 36,
        "type": "text",
        "text": "ui_$_audioSamples_buffer",
        "condition": "(showAdvancedOptions.value && showDescriptions.value)"
    },
    "audioSamples_normalize": {
        "order": 37,
        "type": "bool",
        "text": "ui_audioSamples_normalize",
        "value": false,
        "condition": "showAdvancedOptions.value && audioSamples_buffer.value > 0"
    },

