        "order": 131,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseType",
        "condition": "showDescriptions.value"
    },
    "barVisualizer_bars_responseProvider": {
        "order": 132,
        "type": "combo",
        "text": "ui_barVisualizer_bars_responseProvider",
        "options": [
            { "label": "Value", "value": "Value" },
            { "label": "Value (normalized)", "value": "NormalizedValue" },
            { "label": "Value Change", "value": "Change" }
        ],
        "value": "NormalizedValue",
        "condition": "showAdvancedOptions.value && barVisualizer_bars_responseType.value !== 'None'"
    },
    "$_barVisualizer_bars_responseProvider": {
        "order": 133,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseProvider",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && barVisualizer_bars_responseType.value !== 'None'"
