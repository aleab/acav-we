        "step": 0.1,
        "editable": false,
        "condition": "showAdvancedOptions.value && barVisualizer_bars_responseType.value !== 'None'"
    },
    "barVisualizer_bars_responseRange": {
        "order": 136,
        "type": "slider",
        "text": "ui_barVisualizer_bars_responseRange",
        "value": 25,
        "min": -50,
        "max": 50,
        "fraction": false,
        "editable": false,
        "condition": "['Saturation','HsvBrightness','HslLightness','LabLightness'].includes(barVisualizer_bars_responseType.value)"
    },
    "$_barVisualizer_bars_responseRange": {
        "order": 137,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseRange",
        "condition": "showDescriptions.value && ['Saturation','HsvBrightness','HslLightness','LabLightness'].includes(barVisualizer_bars_responseType.value)"
    },
    "barVisualizer_bars_responseDegree": {
        "order": 138,
        "type": "slider",
        "text": "ui_barVisualizer_bars_responseDegree",
        "value": 0.8,
        "min": 0.1,
        "max": 3,
        "fraction": true,
        "step": 0.1,
        "editable": false,
        "condition": "showAdvancedOptions.value && barVisualizer_bars_responseType.value !== 'None'"
    },
    "$_barVisualizer_bars_responseDegree": {
        "order": 139,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseDegree",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && barVisualizer_bars_responseType.value !== 'None'"
    },
    "barVisualizer_bars_response_toHue": {
