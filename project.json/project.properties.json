    "barVisualizer_bars_width": {
        "order": 121,
        "type": "slider",
        "text": "ui_barVisualizer_bars_width",
        "value": 38,
        "min": 1,
        "max": 100,
        "fraction": false,
        "editable": false
    },
    "barVisualizer_bars_height": {
        "order": 122,
        "type": "slider",
        "text": "ui_barVisualizer_bars_height",
        "value": 45,
        "min": 10,
        "max": 100,
        "fraction": false,
        "editable": false
    },
    "barVisualizer_bars_alignment": {
        "order": 123,
        "type": "slider",
        "text": "ui_barVisualizer_bars_alignment",
        "value": 0,
        "min": -1,
        "max": 1,
        "fraction": true,
        "step": 0.01,
        "editable": false
    },
    "barVisualizer_bars_color": {
        "order": 124,
        "type": "color",
        "text": "ui_barVisualizer_bars_color",
        "value": "0.305882 0.529411 0.698039"
    },

    "barVisualizer_bars_responseType": {
        "order": 130,
        "type": "combo",
        "text": "ui_barVisualizer_bars_responseType",
        "options": [
            { "label": "None", "value": "None" },
            { "label": "<samp><u>H</u>S*</samp> - Hue", "value": "Hue" },
            { "label": "<samp>H<u>S</u>*</samp> - Saturation", "value": "Saturation" },
            { "label": "<samp>HS<u>V</u></samp> - Brightness", "value": "HsvBrightness" },
            { "label": "<samp>HS<u>L</u></samp> - Lightness", "value": "HslLightness" },
            { "label": "<samp><u>L</u>AB</samp> - Lightness", "value": "LabLightness" }
        ],
        "value": "HsvBrightness"
    },
