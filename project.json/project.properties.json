{
    "schemecolor": {
        "order": 0,
        "text": "ui_browse_properties_scheme_color",
        "type": "color",
        "value": "0 0 0"
    },
    "showDescriptions": {
        "order": 1,
        "type": "bool",
        "text": "ui_showDescriptions",
        "value": true
    },
    "showAdvancedOptions": {
        "order": 2,
        "type": "bool",
        "text": "ui_showAdvancedOptions",
        "value": false
    },
    "showStats": {
        "order": 3,
        "type": "bool",
        "text": "ui_showStats",
        "value": false,
        "condition": "showAdvancedOptions.value"
    },
    "limitFps": {
        "order": 4,
        "type": "bool",
        "text": "ui_limitFps",
        "value": true,
        "condition": "showAdvancedOptions.value"
    },

    // [BACKGROUND]
    "background": {
        "order": 50,
        "type": "text",
        "text": "ui_background"
    },
    "background_type": {
        "order": 51,
        "type": "combo",
        "text": "ui_background_type",
        "options": [
            { "label": "Solid Color", "value": "Color" },
            { "label": "Image", "value": "Image" },
            { "label": "CSS", "value": "Css" },
            { "label": "Playlist", "value": "Playlist" }
        ],
        "value": "Color"
    },
    "background_color": {
        "order": 52,
        "type": "color",
        "text": "ui_background_color",
        "value": "0 0.007843 0.035294",
        "condition": "background_type.value === 'Color'"
    },
    "background_image": {
        "order": 52,
        "type": "file",
        "text": "ui_background_image",
        "condition": "background_type.value === 'Image'"
    },
    "background_css": {
        "order": 52,
        "type": "textinput",
        "text": "ui_background_css",
        "value": "background: radial-gradient(ellipse at center, #0d0f16 0%,#030611 60%,#000105 100%);",
        "condition": "background_type.value === 'Css'"
    },
    "background_$_css": {
        "order": 53,
        "type": "text",
        "text": "ui_background_$_css",
        "condition": "showDescriptions.value && background_type.value === 'Css'"
    },
    "background_playlist": {
        "order": 52,
        "type": "directory",
        "text": "ui_background_playlist",
        "mode": "ondemand",
        "condition": "background_type.value === 'Playlist'"
    },
    "background_playlistTimer": {
        "order": 53,
        "type": "slider",
        "text": "ui_background_playlistTimer",
        "value": 1,
        "min": 0.5,
        "max": 24,
        "fraction": true,
        "step": 0.5,
        "editable": false,
        "condition": "background_type.value === 'Playlist'"
    },

    // [AUDIO SAMPLES]
    "audioSamples": {
        "order": 100,
        "type": "text",
        "text": "ui_audioSamples"
    },
    "$_audioSamples": {
        "order": 101,
        "type": "text",
        "text": "ui_$_audioSamples",
        "condition": "(showAdvancedOptions.value && showDescriptions.value)"
    },
    "audioSamples_correct": {
        "order": 102,
        "type": "bool",
        "text": "ui_audioSamples_correct",
        "value": true,
        "condition": "showAdvancedOptions.value"
    },
    "audioSamples_volumeGain": {
        "order": 104,
        "type": "slider",
        "text": "ui_audioSamples_volumeGain",
        "value": 35,
        "min": -100,
        "max": 100,
        "editable": false
    },
    "audioSamples_freqThreshold": {
        "order": 105,
        "type": "slider",
        "text": "ui_audioSamples_freqThreshold",
        "value": 0.1,
        "min": 0,
        "max": 10,
        "fraction": true,
        "step": 0.1,
        "editable": false
    },

    // [AUDIO SAMPLES > SCALE]
    "audioSamples_scale": {
        "order": 110,
        "type": "combo",
        "text": "ui_audioSamples_scale",
        "options": [
            { "label": "ui_audioSamples_scale_Linear", "value": "Linear" },
            { "label": "ui_audioSamples_scale_Power", "value": "Power" },
            { "label": "ui_audioSamples_scale_Exponential", "value": "Exponential" },
            { "label": "ui_audioSamples_scale_Logarithm", "value": "Logarithm" },
            { "label": "ui_audioSamples_scale_Logarithm$Power", "value": "Logarithm$Power" },
            { "label": "ui_audioSamples_scale_Gaussian", "value": "Gaussian" }
        ],
        "value": "Logarithm",
        "condition": "showAdvancedOptions.value"
    },
    "$_audioSamples_scale_Power": {
        "order": 111,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Power",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Power'"
    },
    "audioSamples_scale_Power_exponent": {
        "order": 112,
        "type": "slider",
        "text": "ui_audioSamples_scale_Power_exponent",
        "value": 2,
        "min": 0.2,
        "max": 4,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Power'"
    },
    "$_audioSamples_scale_Exponential": {
        "order": 111,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Exponential",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Exponential'"
    },
    "audioSamples_scale_Exponential_base": {
        "order": 112,
        "type": "slider",
        "text": "ui_audioSamples_scale_Exponential_base",
        "value": 2,
        "min": 1.5,
        "max": 3,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Exponential'"
    },
    "$_audioSamples_scale_Logarithm": {
        "order": 111,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Logarithm",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Logarithm'"
    },
    "audioSamples_scale_Logarithm_base": {
        "order": 112,
        "type": "slider",
        "text": "ui_audioSamples_scale_Logarithm_base",
        "value": 2,
        "min": 1.8,
        "max": 3,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Logarithm'"
    },
    "$_audioSamples_scale_Logarithm$Power": {
        "order": 111,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Logarithm$Power",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Logarithm$Power'"
    },
    "audioSamples_scale_Logarithm$Power_a": {
        "order": 112,
        "type": "slider",
        "text": "ui_audioSamples_scale_Logarithm$Power_a",
        "value": 0.4,
        "min": 0,
        "max": 1,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Logarithm$Power'"
    },
    "audioSamples_scale_Logarithm$Power_base": {
        "order": 113,
        "type": "slider",
        "text": "ui_audioSamples_scale_Logarithm$Power_base",
        "value": 3,
        "min": 1.8,
        "max": 5,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Logarithm$Power'"
    },
    "audioSamples_scale_Logarithm$Power_exponent": {
        "order": 114,
        "type": "slider",
        "text": "ui_audioSamples_scale_Logarithm$Power_exponent",
        "value": 1.8,
        "min": 0.2,
        "max": 5,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Logarithm$Power'"
    },
    "$_audioSamples_scale_Gaussian": {
        "order": 111,
        "type": "text",
        "text": "ui_$_audioSamples_scale_Gaussian",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && audioSamples_scale.value === 'Gaussian'"
    },
    "audioSamples_scale_Gaussian_deviation": {
        "order": 112,
        "type": "slider",
        "text": "ui_audioSamples_scale_Gaussian_deviation",
        "value": 0.43,
        "min": 0.01,
        "max": 0.6,
        "fraction": true,
        "step": 0.1,
        "editable": true,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Gaussian'"
    },
    "audioSamples_scale_Gaussian_mean": {
        "order": 113,
        "type": "slider",
        "text": "ui_audioSamples_scale_Gaussian_mean",
        "value": 0.92,
        "min": 0,
        "max": 1.2,
        "fraction": true,
        "step": 0.1,
        "editable": true,
        "condition": "showAdvancedOptions.value && audioSamples_scale.value == 'Gaussian'"
    },

    // --[AUDIO SAMPLES]
    "audioSamples_buffer": {
        "order": 120,
        "type": "slider",
        "text": "ui_audioSamples_buffer",
        "value": 0.4,
        "min": 0,
        "max": 1,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value"
    },
    "$_audioSamples_buffer": {
        "order": 121,
        "type": "text",
        "text": "ui_$_audioSamples_buffer",
        "condition": "(showAdvancedOptions.value && showDescriptions.value)"
    },
    "audioSamples_normalize": {
        "order": 122,
        "type": "bool",
        "text": "ui_audioSamples_normalize",
        "value": false,
        "condition": "showAdvancedOptions.value && audioSamples_buffer.value > 0"
    },

    // [BAR VISUALIZER]
    "barVisualizer": {
        "order": 1000,
        "type": "text",
        "text": "ui_barVisualizer"
    },
    "barVisualizer_position": {
        "order": 1001,
        "type": "slider",
        "text": "ui_barVisualizer_position",
        "value": 50,
        "min": 0,
        "max": 100,
        "fraction": true,
        "step": 0.1,
        "editable": false
    },
    "barVisualizer_width": {
        "order": 1002,
        "type": "slider",
        "text": "ui_barVisualizer_width",
        "value": 45,
        "min": 25,
        "max": 100,
        "editable": false
    },
    "barVisualizer_flipFrequencies": {
        "order": 1003,
        "type": "bool",
        "text": "ui_barVisualizer_flipFrequencies",
        "value": false,
        "condition": "showAdvancedOptions.value"
    },
    "barVisualizer_smoothing": {
        "order": 1004,
        "type": "slider",
        "text": "ui_barVisualizer_smoothing",
        "value": 38,
        "min": 0,
        "max": 100,
        "condition": "showAdvancedOptions.value && audioSamples_buffer.value > 0"
    },

    // [BAR VISUALIZER > BARS]
    "barVisualizer_bars": {
        "order": 1020,
        "type": "text",
        "text": "ui_barVisualizer_bars"
    },
    "barVisualizer_bars_width": {
        "order": 1021,
        "type": "slider",
        "text": "ui_barVisualizer_bars_width",
        "value": 38,
        "min": 1,
        "max": 100,
        "editable": false
    },
    "barVisualizer_bars_height": {
        "order": 1022,
        "type": "slider",
        "text": "ui_barVisualizer_bars_height",
        "value": 45,
        "min": 10,
        "max": 100,
        "editable": false
    },
    "barVisualizer_bars_borderRadius": {
        "order": 1023,
        "type": "slider",
        "text": "ui_barVisualizer_bars_borderRadius",
        "value": 0,
        "min": 0,
        "max": 100,
        "editable": false
    },
    "barVisualizer_bars_alignment": {
        "order": 1024,
        "type": "slider",
        "text": "ui_barVisualizer_bars_alignment",
        "value": 0,
        "min": -1,
        "max": 1,
        "fraction": true,
        "step": 0.01,
        "editable": false
    },
    "barVisualizer_bars_color": {
        "order": 1025,
        "type": "color",
        "text": "ui_barVisualizer_bars_color",
        "value": "0.176470 0.266667 0.611764"
    },

    "barVisualizer_bars_responseType": {
        "order": 1050,
        "type": "combo",
        "text": "ui_barVisualizer_bars_responseType",
        "options": [
            { "label": "None", "value": "None" },
            { "label": "<samp><u>H</u>S*</samp> - Hue", "value": "Hue" },
            { "label": "<samp>H<u>S</u>*</samp> - Saturation", "value": "Saturation" },
            { "label": "<samp>HS<u>V</u></samp> - Brightness", "value": "HsvBrightness" },
            { "label": "<samp>HS<u>L</u></samp> - Lightness", "value": "HslLightness" },
            { "label": "<samp><u>L</u>AB</samp> - Lightness", "value": "LabLightness" },
            { "label": "<samp>L<u>A</u>B</samp> - Green&#8596;Red", "value": "LabA" },
            { "label": "<samp>LA<u>B</u></samp> - Blue&#8596;Yellow", "value": "LabB" }
        ],
        "value": "HsvBrightness"
    },
    "$_barVisualizer_bars_responseType": {
        "order": 1051,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseType",
        "condition": "showDescriptions.value"
    },
    "barVisualizer_bars_responseProvider": {
        "order": 1052,
        "type": "combo",
        "text": "ui_barVisualizer_bars_responseProvider",
        "options": [
            { "label": "Value", "value": "Value" },
            { "label": "Value (normalized)", "value": "ValueNormalized" },
            { "label": "Change", "value": "Change" },
            { "label": "Change (absolute)", "value": "ChangeAbsolute" }
        ],
        "value": "ValueNormalized",
        "condition": "showAdvancedOptions.value && barVisualizer_bars_responseType.value !== 'None'"
    },
    "$_barVisualizer_bars_responseProvider": {
        "order": 1053,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseProvider",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && barVisualizer_bars_responseType.value !== 'None'"
    },
    "barVisualizer_bars_responseValueGain": {
        "order": 1054,
        "type": "slider",
        "text": "ui_barVisualizer_bars_responseValueGain",
        "value": 1,
        "min": 0.1,
        "max": 5,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && barVisualizer_bars_responseType.value !== 'None'"
    },
    "barVisualizer_bars_responseRange": {
        "order": 1056,
        "type": "slider",
        "text": "ui_barVisualizer_bars_responseRange",
        "value": 25,
        "min": -50,
        "max": 50,
        "condition": "!['None','Hue'].includes(barVisualizer_bars_responseType.value)"
    },
    "$_barVisualizer_bars_responseRange": {
        "order": 1057,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseRange",
        "condition": "showDescriptions.value && !['None','Hue'].includes(barVisualizer_bars_responseType.value)"
    },
    "barVisualizer_bars_responseDegree": {
        "order": 1058,
        "type": "slider",
        "text": "ui_barVisualizer_bars_responseDegree",
        "value": 0.8,
        "min": 0.1,
        "max": 3,
        "fraction": true,
        "step": 0.1,
        "condition": "showAdvancedOptions.value && barVisualizer_bars_responseType.value !== 'None'"
    },
    "$_barVisualizer_bars_responseDegree": {
        "order": 1059,
        "type": "text",
        "text": "ui_$_barVisualizer_bars_responseDegree",
        "condition": "(showAdvancedOptions.value && showDescriptions.value) && barVisualizer_bars_responseType.value !== 'None'"
    },
    "barVisualizer_bars_response_toHue": {
        "order": 1060,
        "type": "color",
        "text": "ui_barVisualizer_bars_response_toHue",
        "value": "0.4 0.839 0.8",
        "condition": "barVisualizer_bars_responseType.value === 'Hue'"
    },

    // [NOTES]
    "notes": {
        "order": 9000,
        "type": "text",
        "text": "ui_notes"
    },
    "note1": {
        "order": 9001,
        "type": "text",
        "text": "ui_note1",
        "condition": "showAdvancedOptions.value"
    },
    "note2": {
        "order": 9002,
        "type": "text",
        "text": "ui_note2"
    }
}
